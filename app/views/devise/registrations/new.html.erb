<div class="card_sign-in-up">
  <h2>Register</h2><br>
  <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>

    <div class="field">
      <%= f.email_field :email, autofocus: true, placeholder:"example@email.com", class: "form-control" %><br>
      <%= f.password_field :password, autocomplete: "off", placeholder:"Password (6 chars minimum)", class: "form-control" %><br>
      <%= f.password_field :password_confirmation, autocomplete: "off", placeholder:"Password confirmation", class: "form-control" %>
    </div>

    <div class="actions"><br>
      <%= f.submit "Register", class: 'btn btn-lg btn-primary btn-block' %>
    </div>
  <% end %>

  <%#= render "devise/shared/links" %>
</div>
