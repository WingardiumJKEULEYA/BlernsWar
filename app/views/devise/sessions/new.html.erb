<div class="card_sign-in-up">
  <h2>Log in</h2><br>
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="field">
      <%= f.email_field :email, autofocus: true, placeholder:"example@email.com", class: "form-control", id: 'email_field' %>
      <%= f.password_field :password, autocomplete: "off", placeholder:"Password", class: "form-control", id: 'password_field' %>
    </div>

    <% if devise_mapping.rememberable? -%>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    <% end -%>

    <div class="actions"><br>
      <%= f.submit "Log in", class: 'btn btn-lg btn-primary btn-block' %>
    </div>
  <% end %>

  <%#= render "devise/shared/links" %>
</div>
